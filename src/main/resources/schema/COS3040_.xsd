<!-- Definicao de campos de identificacao -->
<xs:simpleType name='tipoCodCliente'>
  <xs:restriction base='xs:string'>
    <xs:whiteSpace value='collapse'/>
    <xs:pattern value='[0-9A-Za-z]+'/>
    <xs:maxLength value='14'/>
    <xs:minLength value='1'/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name='tipoCodContrato'>
  <xs:restriction base='xs:string'>
    <xs:pattern value='[^\r\n\|]+'/>
    <xs:minLength value='1'/>
    <xs:maxLength value='40'/>
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name='tipoCodConglomerado'>
  <xs:restriction base='xs:string'>
    <xs:whiteSpace value='collapse'/>
    <xs:pattern value='[^\r\n\|]+'/>
    <xs:maxLength value='40'/>
    <xs:minLength value='1'/>
  </xs:restriction>
</xs:simpleType>

