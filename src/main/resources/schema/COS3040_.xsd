<!-- ******************************* -->
<!-- Definicao dos elementos simples -->
<!-- ******************************* -->

<!-- tipos Doc3040 -->
 <xs:element name='Doc3040' type='tipo3040'/>
 <xs:element name='Cli' type='tipoCliente'/>
 <xs:element name='Op' type='tipoOperacao'/>
 <xs:element name='Agreg' type='tipoAgregacao'/>
 <xs:element name='Venc' type='tipoVencimento'/>
 <xs:element name="Gar" type="tipoGarantia"/>
 <xs:element name="Inf" type="tipoInfoAdicional"/>

<!-- ****************** -->
<!-- Definicao de tipos -->
<!-- ****************** -->

<!-- Documento 3040 -->
<xs:complexType name='tipo3040'>
  <xs:sequence>
    <xs:choice minOccurs='0' maxOccurs='unbounded'>
      <xs:element ref='Cli' minOccurs='0' maxOccurs='unbounded'/>
      <xs:element ref='Agreg' minOccurs='0' maxOccurs='unbounded'/>
    </xs:choice>
  </xs:sequence>
  <xs:attribute name='DtBase' type='tipoDataMesAno' use='required'/>
  <xs:attribute name='CNPJ' type='tipoCNPJ8' use='required'/>
  <xs:attribute name='Parte' type='tipoParteRemessa' use='required'/>
  <xs:attribute name='Remessa' type='tipoParteRemessa' use='required'/>
  <xs:attribute name='TpArq' type='tipoArquivo' use='optional'/>  
  <xs:attribute name='NomeResp' type='tipoNomeResp' use='required'/>
  <xs:attribute name='EmailResp' type='tipoEmailResp' use='required'/>
  <xs:attribute name='TelResp' type='tipoTelResp' use='required'/>
  <xs:attribute name='TotalCli' type='tipoQuantidadeComZero' use='optional'/>
</xs:complexType>

<!-- Cliente -->
<xs:complexType name='tipoCliente'>
 <xs:sequence>
   <xs:element ref='Op' minOccurs='0' maxOccurs='unbounded'/>
 </xs:sequence>
 <xs:attribute name='Cd' type='tipoCodCliente' use='required'/>
 <xs:attribute name='Tp' type='tipoDoCliente' use='required'/>
 <xs:attribute name='IniRelactCli' type='tipoData' use='required'/>
 <xs:attribute name='Autorzc' type='tipoSimNao' use='required'/>
 <xs:attribute name='FatAnual' type='tipoValor' use='optional'/>
 <xs:attribute name='PorteCli' type='tipoPorteCliente' use='optional'/>
 <xs:attribute name='TpCtrl' type='tipoControle' use='optional'/>
 <xs:attribute name='CongEcon' type='tipoCodConglomerado' use='optional'/>
 <xs:attribute name='ClassCli' type='tipoClassCliente' use='required'/>
</xs:complexType>

<!-- Operacao -->
<xs:complexType name='tipoOperacao'>
  <xs:sequence>
   <xs:element ref='Venc' minOccurs='0' maxOccurs='1'/>
   <xs:element ref="Gar" minOccurs="0" maxOccurs="unbounded"/>
   <xs:element ref="Inf" minOccurs="0" maxOccurs="unbounded"/>
  </xs:sequence>
 <xs:attribute name='DetCli' type='tipoCNPJ14' use='optional'/>
 <xs:attribute name='Contrt' type='tipoCodContrato' use='required'/>
 <xs:attribute name='Mod' type='tipoModalidade' use='required'/>
 <xs:attribute name='Cosif' type='tipoContaCosif' use='required'/>
 <xs:attribute name='OrigemRec' type='tipoOrigemRec' use='required'/>
 <xs:attribute name='Indx' type='tipoIndexador' use='required'/>
 <xs:attribute name='PercIndx' type='tipoTaxa' use='optional'/>
 <xs:attribute name='VarCamb' type='tipoVariacaoCambial' use='required'/>
 <xs:attribute name='CEP' type='tipoCEP' use='required'/>
 <xs:attribute name='TaxEft' type='tipoTaxa' use='required'/>
 <xs:attribute name='DtContr' type='tipoData' use='required'/>
 <xs:attribute name='VlrContr' type='tipoValorComZero' use='optional'/>
 <xs:attribute name='NatuOp' type='tipoNaturezaIndiv' use='required'/>
 <xs:attribute name='DtVencOp' type='tipoData' use='optional'/>
 <xs:attribute name='ClassOp' type='tipoClassificacao' use='optional'/>
 <xs:attribute name='ProvConsttd' type='tipoValorComZero' use='optional'/> 
 <xs:attribute name='DiaAtraso' type='tipoQuantidadeComZero' use='optional'/> 
 <xs:attribute name='CaracEspecial' type='tipoCaracEspecial' use='optional'/>
</xs:complexType>

<!-- Informacao adicional -->
<xs:complexType name="tipoInfoAdicional">
    <xs:attribute name="Tp" type="tipoDaInfoAdicional" use="required"/>
    <xs:attribute name="Cd" type="tipoCodContrato" use="optional"/>
    <xs:attribute name="Ident" type="tipoIdentInf" use="optional"/>
    <xs:attribute name="Valor" type="tipoValorComZero" use="optional"/>
    <xs:attribute name="Perc" type="tipoPorcentagemComZero" use="optional"/>
    <xs:attribute name="Qtd" type="tipoQuantidade" use="optional"/>
</xs:complexType>

<!-- Garantia -->
<xs:complexType name="tipoGarantia">
    <xs:attribute name="Tp" type="tipoDeGarantia" use="required"/>
    <xs:attribute name="Ident" type="tipoCodCliente" use="optional"/>
    <xs:attribute name="PercGar" type="tipoPorcentagem" use="optional"/>
    <xs:attribute name="VlrOrig" type="tipoValor" use="optional"/>
    <xs:attribute name="VlrData" type="tipoValorComZero" use="optional"/>
    <xs:attribute name="DtReav" type="tipoData" use="optional"/>
</xs:complexType>

<!-- Agregacao (antigo 3030) -->
<xs:complexType name='tipoAgregacao'>
 <xs:sequence>
  <xs:element ref='Venc' minOccurs='1' maxOccurs='1'/>
 </xs:sequence>
 <xs:attribute name='NatuOp' type='tipoNaturezaAgreg'  use='required'/>
