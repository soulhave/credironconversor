 <xs:attribute name='PrzProvm' type='tipoSimNao' use='optional'/>
 <xs:attribute name='Localiz' type='tipoLocalizacao' use='required'/>
 <xs:attribute name='TpCli' type="tipoDoCliente" use='required'/>
 <xs:attribute name='TpCtrl' type='tipoControle' use='optional'/>
 <xs:attribute name='DesempOp' type='tipoDesempOp' use='required'/>
 <xs:attribute name='CaracEspecial' type='tipoCaracEspecialAgreg' use='optional'/>
 <xs:attribute name='ProvConsttd' type='tipoValorComZero' use='required'/>
